import java.util.Scanner;

class Alphabet
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);

		System.out.print("Enter a number: ");
		int n = sc.nextInt();

		char ch = 'a';

		for (int i = 0; i < n; i++)
		{
			for (int j = 0; j < n; j++)
			{
				System.out.print(ch++ + " ");
			}
			System.out.println();
		}
	}
}
