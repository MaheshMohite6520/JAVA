class StarName 
{
	public static void main(String[] args) 
	{
		int n = 15;

		for (int i = 0; i < n; i++)
		{
			for (int m = 0; m < n; m++)
			{
				if (m == 0 || m == n-1 || (i == m || i + m == n - 1) && i <= n/2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.print(" ");

			for (int a = 0; a < n; a++)
			{
				if (a == 0 || a == n-1 || i == 0 || i == n / 2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.print(" ");

			for (int h = 0; h < n; h++)
			{
				if (h == 0 || h == n-1 || i == n/2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.print(" ");

			for (int e = 0; e < n; e++)
			{
				if (e == 0 || i == 0 || i == n-1 || i == n/2 && e <= n/2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.print(" ");

			for (int s = 0; s < n; s++)
			{
				if (i == 0 || i == n-1 || i == n/2 || s == 0 && i <= n/2 || s == n-1 && i >= n/2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.print(" ");

			for (int h2 = 0; h2 < n; h2++)
			{
				if (h2 == 0 || h2 == n-1 || i == n/2)
				{
					System.out.print("*" + " ");
				}
				else
				{
					System.out.print(" " + " ");
				}
			}

			System.out.println();
		}
	}
}
