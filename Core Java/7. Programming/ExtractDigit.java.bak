import java.util.Scanner;

class ExtractDigit 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);

		System.out.print("Enter a number: ");
		int num = sc.nextInt();

		System.out.println("The digits of number " + num + " are: ");
		while(num > 0)
		{
			int last = num % 10;
			System.out.println(last);
			num /= 10;
		}
	}
}
