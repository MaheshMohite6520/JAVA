class PronicNumberRange 
{
	public static void main(String[] args) 
	{
		int start = 1;
		int end = 100;

		while (start <= end)
		{
			boolean flag = false;

			for (int i = 0; i < start / 2; i++)
			{
				if (i * (i + 1) == start)
				{
					System.out.print(i + " * " + (i+1) + " = ");

					flag = true;
					break;
				}
			}

			if (flag == true)
			{
				System.out.println(start);
			}

			start++;
		}
	}
}
