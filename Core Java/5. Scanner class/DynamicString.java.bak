import java.util.Scanner;

class DynamicString
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);

		System.out.print("Enter first string: ");
		String s1 = sc.nextLine();

		System.out.print("Enter first string: ");
		String s2 = sc.nextLine();

		String sum = s1 + s2;
		System.out.println("Sum: " +sum);
	}
}
