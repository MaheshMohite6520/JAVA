import java.util.Scanner;

class DynamicShort
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);

		System.out.println("Enter first number: ");
		int num1 = sc.nextShort();

		System.out.println("Enter first number: ");
		int num2 = sc.nextShort();

		int sum = num1 + num2;
		System.out.println("Sum: " +sum);
	}
}
